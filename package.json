    "install-all": "npm install && cd frontend && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "chart.js": "^4.4.8",
    "react-chartjs-2": "^5.3.0"
  }
}
